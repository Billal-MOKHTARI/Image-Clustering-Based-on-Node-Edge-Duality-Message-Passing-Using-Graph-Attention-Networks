{
  "use_custom_loss": true,
  "model_args": {
    "layer_sizes": [4096, 1024, 512, 256],
    "loss": "MeanCosineDistance",
    "loss_coeffs": [1, 0.8, 0.5],
    "loss_args": {}
  },
  "embeddings": { "path": "data/embeddings/vgg19", "from_run_metadata": false },
  "row_index": {
    "path": "data/embeddings/row_index",
    "from_run_metadata": false
  },
  "keep": 3,

  "epochs": 1000,
  "optimizer": "Adam",
  "optim_params": {
    "lr": 0.001
  },
  "run": "IGMP10",
  "run_args": {
    "capture_strerr": false,
    "capture_stdout": false,
    "capture_hardware_metrics": false,
    "capture_traceback": false
  },
  "from_annotation_matrix": "../../benchmark/datasets/agadez/csv_files/annotations.csv",
  "log_freq": 10,
  "namespace": "training",
  "checkpoint_namespace": "checkpoints",
  "loss_namespace": "losses",
  "hyperpatameter_namespace": "hyperparameters"
}
