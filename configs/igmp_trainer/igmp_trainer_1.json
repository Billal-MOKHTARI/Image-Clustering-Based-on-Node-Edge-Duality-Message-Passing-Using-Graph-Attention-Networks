{
    "use_custom_loss": false,
    "model_args":
    {
        "layer_sizes": [4096, 1024, 512, 256, 128],
        "loss": "MSELoss",
        "loss_coeffs": [1, 1, 1, 1],
        "loss_args": {
            "reduction": "sum"
        }
    },
    "embeddings": "data/embeddings/vgg19",
    "row_index": "data/embeddings/row_index",
    "epochs": 1000,
    "optimizer": "Adam",
    "optim_params": {
        "lr": 0.001
    },
    "run": "Image GAT Message Passing",
    "run_args":
    {
        "capture_strerr": false,
        "capture_stdout": false,
        "capture_hardware_metrics": false,
        "capture_traceback": true
    },
    "from_annotation_matrix": "../../benchmark/datasets/agadez/csv_files/annotations.csv",
    "log_freq": 10,
    "namespace": "training",
    "checkpoint_namespace": "checkpoints",
    "loss_namespace": "losses",
    "hyperpatameter_namespace": "hyperparameters"
}